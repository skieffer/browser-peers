{
  "manifest_version": 2,
  "name": "Window Group Demo Extension",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "*://localhost/*"
  ]
}
